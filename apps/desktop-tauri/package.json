{
    "name": "@cuemate/desktop",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "tauri": "tauri",
        "tauri:dev": "tauri dev",
        "tauri:build": "tauri build",
        "tauri:build:mac-arm64": "arch -arm64 pnpm dlx @tauri-apps/cli-darwin-arm64@1.6.3 tauri build --target aarch64-apple-darwin",
        "tauri:build:mac-x64": "pnpm dlx @tauri-apps/cli-darwin-x64@1.6.3 tauri build --target x86_64-apple-darwin",
        "tauri:build:win": "pnpm dlx @tauri-apps/cli-win32-x64-msvc@1.6.3 tauri build --target x86_64-pc-windows-msvc",
        "lint": "eslint src --ext ts,tsx",
        "format": "prettier --write src"
    },
    "dependencies": {
        "@tauri-apps/api": "^1.6.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "zustand": "^4.5.5",
        "socket.io-client": "^4.7.5",
        "clsx": "^2.1.1",
        "react-hot-toast": "^2.4.1",
        "framer-motion": "^11.0.23",
        "lucide-react": "^0.454.0"
    },
    "devDependencies": {
        "@tauri-apps/cli": "^1.6.3",
        "@tauri-apps/cli-darwin-arm64": "1.6.3",
        "@rollup/rollup-darwin-arm64": "4.46.2",
        "@types/react": "^18.3.11",
        "@types/react-dom": "^18.3.1",
        "@vitejs/plugin-react": "^4.3.4",
        "autoprefixer": "^10.4.20",
        "postcss": "^8.4.41",
        "tailwindcss": "^3.4.14",
        "typescript": "^5.5.4",
        "vite": "^5.4.10"
    }
}