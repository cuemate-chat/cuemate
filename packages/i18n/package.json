{
    "name": "@cuemate/i18n",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./translator": {
            "types": "./dist/translator.d.ts",
            "default": "./dist/translator.js"
        },
        "./locales/*": {
            "types": "./dist/locales/*.d.ts",
            "default": "./dist/locales/*.js"
        }
    },
    "scripts": {
        "build": "tsc",
        "dev": "tsx watch src/index.ts",
        "clean": "rimraf dist"
    },
    "dependencies": {},
    "devDependencies": {
        "tsx": "^4.19.1",
        "typescript": "^5.5.4",
        "@types/node": "^20.16.10",
        "rimraf": "^6.0.1"
    }
}
