{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173"
  },
  "bundle": {
    "active": true,
    "targets": "all"
  },
  "productName": "CueMate Audio Client",
  "mainBinaryName": "CueMate Audio Client",
  "version": "0.1.0",
  "identifier": "com.cuemate.desktop-client",
  "plugins": {},
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "CueMate Audio Client",
        "width": 800,
        "height": 600,
        "minWidth": 600,
        "minHeight": 400,
        "useHttpsScheme": true
      }
    ],
    "security": {
      "csp": null
    }
  }
}